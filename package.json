{
  "name": "n8n-nodes-genie-dial",
  "version": "1.0.2",
  "description": "n8n node for GenieDial AI calling functionality",
  "keywords": [
    "n8n-community-node-package",
    "geniedial",
    "ai-calling",
    "voice",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://geniedial.in",
  "author": {
    "name": "GenieDial",
    "email": "support@geniedial.in"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nrvmodi/n8n-nodes-genie-dial.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" --ext .ts",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GenieDialApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/GenieDial/GenieDial.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
